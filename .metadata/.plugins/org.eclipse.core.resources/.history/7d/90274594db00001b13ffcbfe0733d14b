package com.syntax.class02;

public class VariableValues {

	public static void main(String[] args) {

		int mynum1;
		float floatnum;
		char var1;
		
		mynum1= 10;
		floatnum = 10.61;
		var1= 'A';
		
		System.out.println(mynum1);
		System.out.println(floatnum);
		System.out.println(var1);
		
		
		
		

		

	}

}
